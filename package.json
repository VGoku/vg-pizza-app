{
  "name": "vg-pizza-app",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "npm run dev --workspace=frontend",
    "dev:backend": "npm run dev --workspace=backend",
    "build": "npm run build --workspaces",
    "test": "npm run test --workspaces"
  },
  "devDependencies": {
    "@types/node": "^18.19.0",
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "axios": "^1.8.4",
    "eslint": "^9.24.0",
    "jest": "^29.7.0",
    "react-icons": "^5.5.0",
    "rimraf": "^5.0.5",
    "uuid": "^11.1.0",
    "zustand": "^5.0.3"
  },
  "resolutions": {
    "glob": "^10.0.0",
    "rimraf": "^5.0.5"
  },
  "overrides": {
    "glob": "^10.3.10",
    "rimraf": "^5.0.5",
    "superagent": "^9.0.1"
  }
}
